<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="custom_icon" format="integer" />
    <attr name="custom_name" format="string" />
    <attr name="font_name" format="string" />
    

    <declare-styleable name="AirIndicatorCustomView">
        <attr name="progress_position" format="integer" />
    </declare-styleable>


    <declare-styleable name="CustomTextView">
        <attr name="font_name" />
    </declare-styleable>
    <declare-styleable name="CustomEditTextView">
        <attr name="font_name" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="font_name" />
    </declare-styleable>

    <declare-styleable name="SunInDayCustom">
        <attr name="start_time" format="string" />
        <attr name="end_time" format="string" />
        <attr name="now_time" format="string" />
    </declare-styleable>

    <declare-styleable name="MoonCustomView">
        <attr name="percent_moon" format="string" />
        <attr name="strokeMoon" format="reference" />
        <attr name="moonInsideColor" format="reference" />
        <attr name="textColorDay" format="reference"/>
        <attr name="textColorMoonStatus" format="reference"/>
    </declare-styleable>


    <attr name="backgroundApp" format="integer" />
    <attr name="textColorApp" format="reference" />
    <attr name="textColorSecond" format="reference"/>

    <declare-styleable name="weatherDetail">
        <attr name="bgCardColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="hourDetail">
        <attr name="bgCardSelected" format="reference" />
        <attr name="textInCard" format="reference"/>
    </declare-styleable>

    <declare-styleable name="dayDetail">
        <attr name="rainTextColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="sunDetail">
        <attr name="backgroundSun" format="integer" />
    </declare-styleable>

    <declare-styleable name="windDetail">
        <attr name="bgWind" format="reference"/>
        <attr name="tintColor" format="reference"/>
    </declare-styleable>



</resources>