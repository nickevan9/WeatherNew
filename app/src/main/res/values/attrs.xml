    <attr name="font_name" format="string" />
    

    <declare-styleable name="AirIndicatorCustomView">
        <attr name="progress_position" format="integer" />
    </declare-styleable>


    <declare-styleable name="CustomTextView">
        <attr name="font_name" />
    </declare-styleable>
    <declare-styleable name="CustomEditTextView">
        <attr name="font_name" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="font_name" />
    </declare-styleable>

    <declare-styleable name="SunInDayCustom">
        <attr name="start_time" format="string" />
        <attr name="end_time" format="string" />
